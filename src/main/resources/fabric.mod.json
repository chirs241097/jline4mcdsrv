{
  "schemaVersion": 1,
  "id": "jline4mcdsrv",
  "version": "${version}",

  "name": "JLine for Minecraft Dedicated Server",
  "description": "Enables command history, auto completion and syntax highlighting on the server console.",
  "authors": [
    "Chris Xiong",
    "Fourmisain"
  ],
  "contact": {
    "homepage": "https://chrisoft.org/",
    "sources": "https://github.com/chirs241097/jline4mcdsrv"
  },

  "license": "MIT",
  "icon": "assets/jline4mcdsrv/icon.png",

  "environment": "server",
  "entrypoints": {
    "main": [
      "org.chrisoft.jline4mcdsrv.JLineForMcDSrvMain"
    ]
  },
  "mixins": [
    "jline4mcdsrv.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.12.12",
    "minecraft": ">=1.17"
  },
  "suggests": {
    "flamingo": "*"
  }
